n = int(raw_input())
child = [[] for i in xrange(n)]
for loop in xrange(n - 1):
    a, b = map(int, raw_input().split())
    a -= 1; b -= 1
    child[a].append(b)

s = mx = 0
for i in xrange(n):
    if len(child[i]) > mx:
        mx = len(child[i])
        s = i

